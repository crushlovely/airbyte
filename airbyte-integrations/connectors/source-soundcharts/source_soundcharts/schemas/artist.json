{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": ["uuid", "slug", "name"],
  "properties": {
    "uuid": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "appUrl": {
      "type": "string"
    },
    "imageUrl": {
      "type": ["null", "string"]
    },
    "countryCode": {
      "type": ["null", "string"]
    }
  }
}
